<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yrc.emos.wx.mapper.PermissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yrc.emos.wx.pojo.entity.PermissionEntity">
        <id column="id" property="id" />
        <result column="permission_name" property="permissionName" />
        <result column="module_id" property="moduleId" />
        <result column="action_id" property="actionId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, permission_name, module_id, action_id
    </sql>

</mapper>
